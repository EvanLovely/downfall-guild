{% include "@organisms/site/site-nav/site-nav.twig" %}
{% include "@molecules/headers/header.twig" with header %}

<div class="main main-raised">

  {% block content %}
    <h2>Site Content Here</h2>
  {% endblock %}
</div> <!-- /.main -->

<div class="subsection">
  {% block sub %}

    {% embed '@organisms/layouts/section.twig' with { name: 'raid-activity' } %}
      {% block section_content %}
        <div class="row">
          <div class="col-sm-12">
            <h2 class="title text-center">Downfall Raid Stuff</h2>
            <br>
          </div>
          <div class="col-sm-4">
            {% include "@molecules/cards/card-white.twig" with subsection.left %}
          </div>
          <div class="col-sm-4">
            {% include "@molecules/cards/card-white.twig" with subsection.middle %}
          </div>
          <div class="col-sm-4">
            {% include "@molecules/cards/card-white.twig" with subsection.right %}
          </div>
        </div>
      {% endblock %}
    {% endembed %}
  {% endblock %}
</div>

{% include "@organisms/site/site-footer/site-footer.twig" %}
